#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules/esbuild/bin/node_modules:/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules/esbuild/node_modules:/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules:/Users/ryanprendergast/.openclaw/workspace/node_modules:/Users/ryanprendergast/.openclaw/node_modules:/Users/ryanprendergast/node_modules:/Users/node_modules:/node_modules:/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules/esbuild/bin/node_modules:/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules/esbuild/node_modules:/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules:/Users/ryanprendergast/.openclaw/workspace/node_modules:/Users/ryanprendergast/.openclaw/node_modules:/Users/ryanprendergast/node_modules:/Users/node_modules:/node_modules:/Users/ryanprendergast/.openclaw/workspace/code-rl-briefing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
